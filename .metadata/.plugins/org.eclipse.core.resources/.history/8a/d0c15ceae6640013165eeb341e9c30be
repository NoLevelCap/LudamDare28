package com.nolevelcap.player.info;

import java.util.ArrayList;

public class Powers {
	
	public Power currentPower;
	public ArrayList<Power> allPowers;
	
	public Powers(){
	}
	
	public void initPowers(){
		Power HolySlimes = new HolySlimes(0, 1);
		allPowers.add(HolySlimes);
		
		Power ResearchContraption = new ResearchContraption(1, 1);
		allPowers.add(ResearchContraption);
		
		Power Gore2Gold = new Gore2Gold(2, 1);
		allPowers.add(Gore2Gold);
		
		Power FountainOfYouth = new FountainOfYouth(3, 2);
		allPowers.add(FountainOfYouth);
		
		Power SuperStar = new FountainOfYouth(0, 2);
		allPowers.add(SuperStar);
		
		Power HolySlimes3 = new HolySlimes(0, 2);
		allPowers.add(HolySlimes3);
		
		Power HolySlimes = new HolySlimes(0, 3);
		allPowers.add(HolySlimes);
		
		Power HolySlimes = new HolySlimes(0, 3);
		allPowers.add(HolySlimes);
		
		Power HolySlimes = new HolySlimes(0, 3);
		allPowers.add(HolySlimes);
	}
}
